generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["prismaSchemaFolder"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum MediaType {
  VIDEO
  IMAGE

  @@map("media_type")
}

enum UserRole {
  MENTOR
  USER

  @@map("user_role")
}

enum AuthProviders {
  google
  email

  @@map("auth_providers")
}

enum PassType {
  single
  duo
  squad
  team
  full
}

enum BookingStatus {
  pending
  paid
  cancelled

  @@map("booking_status")
}